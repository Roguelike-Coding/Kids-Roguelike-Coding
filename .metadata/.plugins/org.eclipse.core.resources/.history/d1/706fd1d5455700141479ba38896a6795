package com;

import javax.swing.Action;

public class InventorySlot extends ItemEntity {
	private int stack;
	public InventorySlot(String name, String path, boolean inInventory,Action action) {
		super(name, path, inInventory, action);
		// TODO Auto-generated constructor stub
	}
	public int getStack() {
		return stack;
	}
	public void setStack(int stack) {
		this.stack = stack;
	}

}
